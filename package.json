{
	"name": "n8n-nodes-telegram-getupdates-api",
	"version": "1.4.0",
	"publishConfig": {
		"provenance": true
	},
	"description": "Receive incoming Telegram updates using long polling.",
	"license": "MIT",
	"homepage": "https://github.com/mani-sh-reddy/n8n-nodes-telegram-getupdates-api",
	"keywords": [
		"n8n-community-node-package",
		"telegram",
		"polling",
		"getupdates",
		"bots"
	],
	"author": {
		"name": "mani-sh-reddy",
		"email": "mani-sh-reddy@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mani-sh-reddy/n8n-nodes-telegram-getupdates-api.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "export N8N_SECURE_COOKIE=false && n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/BotTokenTelegramApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TelegramGetUpdates/TelegramGetUpdates.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "^0.17.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.2",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"semantic-release": "^25.0.2",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}